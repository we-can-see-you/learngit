    "#将空缺的age补充上\n",
    "from sklearn.ensemble import RandomForestRegressor\n",
    "def set_missage(data):\n",
    "    know_age=data[[\"Age\",\"SibSp\",\"Parch\",\"Pclass\"]][data.Age.notna()].as_matrix()#已知年龄的人\n",
    "    unknow_age=data[[\"Age\",\"SibSp\",\"Parch\",\"Pclass\"]][data.Age.isna()].as_matrix()#未知年龄的人\n",
    "    \n",
    "    #构造标签\n",
    "    y=know_age[:,0]#注意know_age[:,0]与know_age[:][0]的区别\n",
    "    #构造训练集\n",
    "    X=know_age[:,1::]\n",
    "    \n",
    "    #丢入随即森林\n",
    "    rfr = RandomForestRegressor(random_state=0, n_estimators=2000, n_jobs=-1)\n",
    "    rfr.fit(X, y)\n",
    "    \n",
    "    #进行预测\n",
    "    predictedAges = rfr.predict(unknow_age[:, 1::])\n",
    "    \n",
    "    #将预测值填充到data中\n",
    "    #data.loc[data.Age.isnull()].Age=predictedAges        #data.loc即检索dataframe data.loc[x,y] x填行索引，y为列索引 \n",
    "    data.loc[ (data.Age.isnull()), 'Age' ] = predictedAges \n",
    "    return data"
   ]
  },
